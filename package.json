{
  "name": "gh-usecases",
  "version": "1.1.2",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "gh-usecases": "./dist/index.js"
  },
  "scripts": {
    "command": "tsx src/index.tsx",
    "dev": "tsx watch src/index.tsx",
    "build": "tsc",
    "prepublish": "yarn build",
    "test": "yarn build && ava",
    "test:watch": "ava --watch",
    "test:coverage": "yarn build && c8 ava",
    "lint": "echo 'Linting...' && exit 0",
    "typecheck": "tsc --noEmit",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags",
    "release:major": "npm version major && git push origin main --tags",
    "prerelease:beta": "npm version prerelease --preid=beta && git push origin --follow-tags",
    "prerelease:alpha": "npm version prerelease --preid=alpha && git push origin --follow-tags"
  },
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.4.0",
    "@octokit/graphql": "^7.0.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@swc/core": "^1.12.0",
    "@swc/register": "^0.1.10",
    "@types/fs-extra": "^11.0.4",
    "@types/ink-spinner": "^3.0.4",
    "@types/node": "^24.0.7",
    "@types/react": "^18.2.0",
    "@types/sinon": "^17.0.4",
    "ava": "^6.4.0",
    "c8": "^10.1.3",
    "sinon": "^21.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  }
}
